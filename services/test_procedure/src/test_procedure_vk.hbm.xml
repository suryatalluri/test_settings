<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.test_settings.test_procedure">
  <class table="`Employee`" name="com.test_settings.test_procedure.Employee" entity-name="com.test_settings.test_procedure.Employee" schema="test_procedure">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="name" type="string">
      <column name="`Name`" length="255" not-null="false" precision="19"/>
    </property>
    <property name="city" type="string">
      <column name="`City`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
</hibernate-mapping>